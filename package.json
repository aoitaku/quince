{
  "name": "quince",
  "description": "quick user interface tool kit for JavaScript.",
  "version": "0.10.0",
  "type": "module",
  "licenses": [
    {
      "type": "zlib",
      "url": "https://opensource.org/licenses/Zlib"
    }
  ],
  "private": true,
  "author": "Taku Aoi<aoitaku@gmail.com>",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/aoitaku/guiness.git"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^8.0.0-beta.0",
    "vitest": "^4.0.17"
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:types && vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "node -e \"import('node:fs').then(({rmSync})=>rmSync('types',{recursive:true,force:true}))\"",
    "typecheck": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "example:basic": "tsx examples/basic.ts",
    "example:canvas": "vite dev examples/canvas"
  }
}
